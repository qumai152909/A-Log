#!/usr/bin/env node

const fs = require('fs');

console.log('start test bin');

const args = process.argv.slice(2);


if (args[0] === 'demo1') {
  require('./demo1/demo1')()
}

if (args[0] === 'demo2') {
  require('./demo1/demo2')()
}

if (args[0] === 'txt') {
  require('./lib/txt')
}

if (args[0] === 'prams') {
  require('./lib/showPrams')
}

if (args[0] === 'upfiles') {
  require('./lib/upfiles')
}
